    "breadth first",
    "preorder",
    "postorder"
  ],
  "dependencies": {},
  "devDependencies": {
    "clone": "^1.0.2",
    "meta-dev": "^0.2.0",
    "pre-commit": "^1.1.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
