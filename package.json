{
  "name": "tree-crawl",
  "version": "0.6.1",
  "description": "n-ary tree traversal library.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/tree-crawl",
  "main": "dist/tree-crawl.node.js",
  "browser": "dist/tree-crawl.browser.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "engines": {
    "node": ">=5"
  },
  "files": [
    "index.js",
    "lib/",
    "dist/"
  ],
  "scripts": {
    "lint": "eslint *.js {lib,test}/**/*.js",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test nyc ava",
    "debug": "cross-env NODE_ENV=debug inspect -r babel-register -r ./test/helpers/fixtures.js --debug-exception node_modules/ava/profile.js",
    "start": "meta dev",
    "docs": "documentation build index.js -f md -o docs/api.md",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "see-coverage": "nyc report --reporter=html && open coverage/index.html",
    "check-coverage": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "bundle": "cross-env NODE_ENV=build rollup -c",
    "minify": "uglifyjs dist/tree-crawl.js -c -m --reserved-file .reserved --mangle-props -o dist/tree-crawl.min.js",
    "build": "npm run bundle && npm run minify",
    "prepublish": "npm run build",
    "precommit": "npm run docs && git add docs",
    "bench": "npm run bundle && node ./perf/bench.js",
    "profile": "npm run bundle && devtool ./perf/profile.js",
    "trace": "npm run bundle && node-irhydra ./perf/trace.js"
  },
  "keywords": [
    "tree",
    "n-ary tree",
    "k-ary tree",
    "n-way tree",
    "multiway tree",
    "rose tree",
    "generic",
    "agnostic",
    "traverse",
    "traversal",
    "walk",
    "visit",
    "visitor",
    "recursive",
    "breadth first",
    "preorder",
    "postorder"
  ],
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/fixtures.js"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "node5"
        ],
        "plugins": [
          "istanbul"
        ]
      },
      "debug": {
        "presets": [
          "node5"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "benchmark": "^2.1.3",
    "clone": "^2.0.0",
    "codecov": "^2.1.0",
    "cross-env": "^3.2.4",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.18.0",
    "husky": "^0.13.3",
    "meta-dev": "^0.4.2",
    "nyc": "^10.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglify-js": "^2.8.18"
  }
}
