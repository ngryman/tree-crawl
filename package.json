{
  "name": "tree-crawl",
  "version": "0.5.0",
  "description": "n-ary tree traversal library.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/tree-crawl",
  "main": "dist/tree-crawl.node.js",
  "browser": "dist/tree-crawl.browser.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js",
    "lib/",
    "dist/"
  ],
  "scripts": {
    "lint": "meta lint",
    "pretest": "npm run lint -s",
    "test": "meta test",
    "dev": "meta dev",
    "coverage": "meta coverage",
    "see-coverage": "meta see-coverage",
    "check-coverage": "meta check-coverage",
    "predocs": "npm run build -s",
    "docs": "meta docs",
    "prebuild": "npm run lint -s",
    "build": "meta build",
    "prepublish": "npm run build -s"
  },
  "precommit": [
    "test",
    "check-coverage",
    "docs"
  ],
  "keywords": [
    "tree",
    "n-ary tree",
    "k-ary tree",
    "n-way tree",
    "multiway tree",
    "rose tree",
    "generic",
    "agnostic",
    "traverse",
    "traversal",
    "walk",
    "visit",
    "visitor",
    "recursive",
    "breadth first",
    "preorder",
    "postorder"
  ],
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "eslintConfig": {
    "extends": "ngryman"
  },
  "dependencies": {},
  "devDependencies": {
    "clone": "^2.0.0",
    "meta-dev": "^0.3.4",
    "pre-commit": "^1.1.3"
  }
}
